@page "{albumName?}"
@model AlbumModel
@{
    ViewData[Constants.PageTitle] = Model.AlbumName;
}

<form method="post" enctype="multipart/form-data">
    <input type="file" required="required" name="files" multiple="multiple" />
    <!--<input type="file" required="required" multiple="multiple" accept="image/*" />-->
    <button asp-page-handler="Upload">Upload Files</button>
</form>
